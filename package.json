{
  "name": "control-asistencia",
  "description": "Control de asitencia empresarial.",
  "version": "0.1.8",
  "author": "Luis Pura",
  "license": "MIT",
  "keywords": [],
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/artxro/control_asistenciaPF.git"
  },
  "scripts": {
    "start": "electron . --disable-gpu",
    "build": "electron-builder build --win --x64 --ia32 --publish never",
    "build32": "electron-builder build --win --ia32 --publish never",
    "deploy": "electron-builder build --win --publish always",
    "compress": "cd .. && tar -zcvf Release/control-asistenciaVF.tar.gz control-asistenciaVF",
    "package": "electron-packager . control-asistencia --overwrite --out=packaged --ignore='^/dist$' --prune --platform=linux --arch=x64 --icon=assets/icons/Logo.icns --disable-gpu"
  },
  "build": {
    "target": "nsis ",
    "icon": "assets/icons/icon.ico",
    "nsis": {
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Control de Asistencia PF"
    }
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "body-parser": "^1.18.3",
    "browser-window": "^0.4.0",
    "child_process": "^1.0.2",
    "colors": "^1.3.3",
    "cryptr": "^4.0.2",
    "dateformat": "^3.0.3",
    "dns": "^0.2.2",
    "electron-fetch": "^1.4.0",
    "electron-log": "^3.0.7",
    "electron-notifications": "^1.0.0",
    "electron-packager": "^14.2.1",
    "electron-reload": "^1.5.0",
    "electron-updater": "^4.0.6",
    "file-encryptor": "^0.1.1",
    "filesize": "^6.1.0",
    "fs": "0.0.1-security",
    "is-base64": "^0.1.0",
    "jquery": "^3.4.1",
    "macaddress": "^0.2.9",
    "min-jquery": "^1.7.0",
    "node-datetime": "^2.1.2",
    "path": "^0.12.7",
    "pg": "^7.10.0",
    "readline": "^1.3.0",
    "remote": "^0.2.6",
    "request": "^2.88.2",
    "require": "^2.4.20",
    "sequelize": "^5.15.1",
    "sha256": "^0.2.0",
    "url": "^0.11.0",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "electron": "^8.0.2",
    "electron-builder": "^22.3.2",
    "electron-packager": "^14.2.1"
  }
}
